{
  "sourceType": "module",
  "env": {
    "es": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "targets": {
              "browsers": ["last 5 Chrome versions", "not dead"]
            },
            "loose": false,
            "modules": false,
            "useBuiltIns": false
          }
        ]
      ]
    },
    "cjs": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "targets": {
              "browsers": [">3%", "not dead"]
            },
            "loose": false,
            "modules": false,
            "useBuiltIns": "entry"
          }
        ]
      ]
    },
    "test": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "targets": {
              "browsers": "maintained node versions"
            }
          }
        ]
      ]
    }
  }
}